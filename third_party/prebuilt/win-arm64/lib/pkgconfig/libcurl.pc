prefix=D:/a/gpt4o-API/gpt4o-API/third_party/prefix/win-arm64
exec_prefix=${prefix}
libdir=D:/a/gpt4o-API/gpt4o-API/third_party/prefix/win-arm64/lib
includedir=${prefix}/include
supported_protocols="DICT FILE FTP FTPS GOPHER GOPHERS HTTP HTTPS IMAP IMAPS IPFS IPNS MQTT POP3 POP3S RTSP SMB SMBS SMTP SMTPS TELNET TFTP"
supported_features="SSL IPv6 UnixSockets libz AsynchDNS Largefile alt-svc HSTS NTLM HTTPS-proxy threadsafe"

Name: libcurl
URL: https://curl.se/
Description: Library to transfer files with ftp, http, etc.
Version: 8.6.0
Libs: -L${libdir} -lcurl  -lmingw32 -lunwind -lmoldname -lmingwex -ladvapi32 -lshell32 -luser32 -lkernel32 -lmingw32 -lunwind -lmoldname -lmingwex -lkernel32 -lws2_32 -lbcrypt D:/a/gpt4o-API/gpt4o-API/third_party/prefix/win-arm64/lib/libssl.a D:/a/gpt4o-API/gpt4o-API/third_party/prefix/win-arm64/lib/libcrypto.a D:/a/gpt4o-API/gpt4o-API/third_party/prefix/win-arm64/lib/libz.a -ladvapi32 -lcrypt32
Libs.private:  -lmingw32 -lunwind -lmoldname -lmingwex -ladvapi32 -lshell32 -luser32 -lkernel32 -lmingw32 -lunwind -lmoldname -lmingwex -lkernel32 -lws2_32 -lbcrypt D:/a/gpt4o-API/gpt4o-API/third_party/prefix/win-arm64/lib/libssl.a D:/a/gpt4o-API/gpt4o-API/third_party/prefix/win-arm64/lib/libcrypto.a D:/a/gpt4o-API/gpt4o-API/third_party/prefix/win-arm64/lib/libz.a -ladvapi32 -lcrypt32
Cflags: -I${includedir} -DCURL_STATICLIB
